@model SignUpDtos

@{
    ViewData["Title"] = "SignUp";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}
<div class="main-agileinfo">
	<div class="agileits-top">
		<div asp-validation-summary="All"></div>
		<form asp-action="SignUp">
				<input class="form-control" asp-for="UserName" placeholder="User Name" required="">
				<span asp-validation-for="UserName" class="text-danger"></span>
				<input class="text" asp-for="FirstName" placeholder="First Name" required="">
			<span asp-validation-for="FirstName" class="text-danger"></span>
			<input class="text" asp-for="LastName" placeholder="Last Name" required="">
			<span asp-validation-for="LastName" class="text-danger"></span>
			<input class="text" asp-for="Email" placeholder="Email" required="" aria-describedby="emailHelp">
			<span asp-validation-for="Email" class="text-danger"></span>
			<input class="text" asp-for="Password" placeholder="Password" type="password" required="" title="Must be at least 6 characters">
			<span asp-validation-for="Password" class="text-danger"></span>
			<input class="text w3lpass" asp-for="ConfirmPassword" placeholder="Confirm Password" type="password" required="">
			<span asp-validation-for="ConfirmPassword" class="text-danger"></span>
			<div class="wthree-text">
				<label class="anim">
					<input asp-for="IsAgree" class="checkbox" required="">
					<span>I Agree To The Terms & Conditions</span>
				</label>
				<div class="clear"> </div>
			</div>
			<input type="submit" value="SIGNUP">
		</form>
		<p>Already have an account? <a asp-action="SignIn">Login Now!</a></p>
	</div>
</div>

@section ValidationSection {
	<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}