@model CreateEmployeeDtos


<h1>Add New Employee</h1>

<br />
<br />

<form asp-action="Edit" enctype="multipart/form-data">
    <div class="row g-3">
        <partial name="CreateAndEditePartialView/_CreateAndEditePartialView" />
        <input asp-for="ImageName" hidden/>
